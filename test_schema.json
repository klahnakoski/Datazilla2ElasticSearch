{
	"settings":{
		"index.number_of_shards":1,
		"index.number_of_replicas":0,
		"index.store.throttle.type":"merge",
		"index.store.throttle.max_bytes_per_sec":"2mb"
	},
	"mappings":{
		"test_results":{
			"dynamic_templates":[
				{"default_moment":{
					"mapping":{"index":"no", "type":"double"},
					"match":"*.moments.s*"
				}},
				{"xperf":{
					"mapping":{"type":"string"},
					"match":"results_xperf.*"
				}}
			],
			"properties":{
				"datazilla":{
					"properties":{
						"id":{"type":"long"},
						"date_loaded":{"type":"long"},
						"error_flag":{"type":"string"},
						"test_run_id":{"type":"long"},
						"processed_flag":{"type":"string"},
						"error_msg":{"type":"string"}
					}
				}
			},
			"_all":{
				"enabled":false
			},
			"_source":{
				"compress":true
			},
			"dynamic" : true
		}
	}
}