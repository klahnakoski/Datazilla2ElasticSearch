{
	"settings":{
		"index.number_of_shards":1,
		"index.number_of_replicas":0,
		"index.store.throttle.type":"merge",
		"index.store.throttle.max_bytes_per_sec":"2mb"
	},
	"mappings":{
		"test_results":{
			"dynamic_templates":[
				{"default_moment":{
					"mapping":{"index":"no", "type":"double"},
					"match":"*.moment.*"
				}},
				{"xperf":{
					"mapping":{"index":"no", "type":"string"},
					"match":"results_xperf.*"
				}}
			],
			"_all":{
				"enabled":false
			},
			"_source":{
				"compress":true
			},
			"dynamic" : true
		}
	}
}