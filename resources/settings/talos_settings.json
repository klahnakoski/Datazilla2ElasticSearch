{
	"elasticsearch" : {
		"host" : "http://localhost",
		"port" : 9200,
		"index" : "talos",
		"type" : "test_results",
		"timeout" : 300,
		"schema_file" : "./resources/schema/test_schema.json",
		"debug": true,
		"batch_size": 100
	},
	"production" : {
		"blob_url" : "https://datazilla.mozilla.org/talos/refdata/objectstore/json_blob",
		"threads" : 20,
		"min" : 5000000,
		"step" : 100000,
		"max_tries":10
	},
	"param" : {
		"schema_file" : "./resources/schema/test_schema.json",
		"output_file" : "./results/recent.tab"
	},
	"debug" : {
		"profile" : false,
		"log" : [
			{
				"class" : "logging.handlers.RotatingFileHandler",
				"filename" : "./results/logs/talos_extract.log",
				"maxBytes" : 10000000,
				"backupCount" : 100,
				"encoding" : "utf8"
			},
			{
				"log_type" : "stream",
				"stream" : "sys.stdout"
			}
		]
	}
}
